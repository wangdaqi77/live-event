<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.data.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LikeLiveDataDemoActivity"/>
        <activity android:name=".NoLossDemoActivity"/>
        <activity android:name=".NoStickyDemoActivity"/>
        <activity android:name=".NoStickyNoLossDemoActivity"/>
        <activity android:name=".mixed.MixedLiveEventDemoActivity"/>
        <activity android:name=".mixed.MixedBackgroundLiveEventDemoActivity"/>
        <activity android:name=".other.SpecifiedThreadToObserveDemoActivity"/>
    </application>

</manifest>